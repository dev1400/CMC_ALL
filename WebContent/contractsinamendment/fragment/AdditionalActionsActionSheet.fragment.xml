<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ActionSheet showCancelButton="true" placement="Top">
		<buttons>
			<Button id="idButtonCancelAmendment" text="{i18n>CancelAmendment}"
				tooltip="{i18n>CancelAmendment}" icon="sap-icon://sys-cancel" press="handleAmendmentCancelPress"
				enabled="false" />
			<Button text="{i18n>DownloadtoExcel}" tooltip="{i18n>DownloadtoExcel}"
				icon="sap-icon://download" press="handleDownLoadToExcelButtonPress" />
			<Button text="{i18n>DefaultParameters}" tooltip="{i18n>DefaultParametersTooltip}"
				icon="sap-icon://action-settings" press="handleDefaultParameterPopup" />
		</buttons>
	</ActionSheet>
</core:FragmentDefinition>