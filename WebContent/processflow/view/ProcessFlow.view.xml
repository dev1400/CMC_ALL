<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="dia.cmc.processflow.view.ProcessFlow"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:z="sap.ca.ui">
	<Page title="Title" xmlns:suite="sap.suite.ui.commons">
		<content>

			<Panel>

				<z:ZoomableScrollContainer zoomable="true">
					<suite:ProcessFlow id="idProcessFlow" zoomLevel="Two"
						scrollable="false" nodes="{processFlowLaneModel>/nodes}" lanes="{processFlowLaneModel>/lanes}"
						headerPress="onHeaderPress">
						<suite:nodes>
							<suite:ProcessFlowNode laneId="{processFlowLaneModel>lane}"
								nodeId="{processFlowLaneModel>id}" title="{pf3>title}" children="{processFlowLaneModel>children}"
								state="{processFlowLaneModel>state}" stateText="{processFlowLaneModel>stateText}" texts="{processFlowLaneModel>texts}"
								highlighted="{processFlowLaneModel>highlighted}" focused="{processFlowLaneModel>focused}" />
						</suite:nodes>
						<suite:lanes>
							<suite:ProcessFlowLaneHeader laneId="{processFlowLaneModel>id}"
								iconSrc="{processFlowLaneModel>icon}" text="{processFlowLaneModel>label}" position="{processFlowLaneModel>position}" />
						</suite:lanes>
					</suite:ProcessFlow>
					<html:br />

				</z:ZoomableScrollContainer>

			</Panel>

		</content>
	</Page>
</core:View>